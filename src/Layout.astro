---
import Header from "@/components/Header.astro";
import Footer from "@/components/Footer.astro";
import "@/styles/global.css";
---

<!doctype html>
<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link
      rel="icon"
      type="image/png"
      href="/icons/favicon-96x96.png"
      sizes="96x96"
    />
    <link rel="icon" type="image/svg+xml" href="/icons/favicon.svg" />
    <link rel="shortcut icon" href="/icons/favicon.ico" />
    <link
      rel="apple-touch-icon"
      sizes="180x180"
      href="/icons/apple-touch-icon.png"
    />
    <meta name="apple-mobile-web-app-title" content="NASA Space Apps" />
    <link rel="manifest" href="/icons/site.webmanifest" />
    <title>NASA Space Apps Challenge Encarnación 2025</title>

    <meta
      name="description"
      content="Únete a nosotros en el NASA Space Apps Challenge ENCARNACIÓN 2025 este 4 y 5 de Octubre"
    />
    <meta
      name="keywords"
      content="NASA, Space Apps, Encarnación, Paraguay, Hackathon, 2025, Ciencia, Tecnología, Innovación"
    />
    <meta name="robots" content="index, follow" />
    <link rel="canonical" href="https://spaceappsencarnacion.live" />

    <!-- OG Meta Tags -->
    <meta
      property="og:image"
      content="https://spaceappsencarnacion.live/images/og.png"
    />
    <meta property="og:image:width" content="1200" />
    <meta property="og:image:height" content="720" />
    <meta
      property="og:title"
      content="NASA SPACE APPS CHALLENGE ENCARNACIÓN 2025"
    />
    <meta
      property="og:description"
      content="¡Inscripción ABIERTA! El evento es GRATUITO y se realizará en el Auditorio Central de la Universidad Nacional de Itapúa. Únete al NASA Space Apps Challenge en Encarnación, Paraguay. Participa en el hackathon global de innovación, ciencia y tecnología este 4 y 5 de octubre de 2025, y demuestra tu talento resolviendo desafíos reales con datos de la NASA."
    />
    <meta property="og:url" content="https://spaceappsencarnacion.live" />

    <script type="application/ld+json" class="schemantra">
      {
        "@context": "https://schema.org",
        "@type": "Hackathon",
        "@id": "NASA Space Apps Challenge Encarnación 2025",
        "additionalType": "[https://spaceappsencarnacion.live/,https://www.spaceappschallenge.org/2025/local-events/Encarnacion/]",
        "alternateName": "Space Apps Encarnación",
        "description": "Hackathon de 48 horas NASA Space Apps Challenge ENCARNACIÓN 2025 únete el 4 y 5 de Octubre. El evento perfecto para que tu talento brille en ciencia, tecnología e innovación",
        "doorTime": "2025-10-04T09:14",
        "endDate": "2025-10-05T23:59",
        "inLanguage": "[Spanish,English]",
        "keywords": "NASA, Space Apps, Encarnación, Paraguay, Hackathon, 2025, Ciencia, Tecnología, Innovación, Gratis, UNI, FIUNI",
        "maximumAttendeeCapacity": "100",
        "maximumPhysicalAttendeeCapacity": "100",
        "maximumVirtualAttendeeCapacity": "0",
        "name": "NASA Space Apps Challenge Encarnación 2025",
        "typicalAgeRange": "15-99",
        "image": "https://spaceappsencarnacion.live/images/og.png"
        "url": "https://spaceappsencarnacion.live/",
        "disambiguatingDescription": "A local event in Encarnacion, Paraguay, held on October 4-5, 2025, as part of the global NASA International Space Apps Challenge—a 48-hour hackathon where teams use NASA and partner space agency open data to solve real-world problems",
        "identifier": "https://spaceappsencarnacion.live/",
        "location": {
          "@type": "Place",
          "name": "Auditorio Central de la Universidad Nacional de Itapúa",
          "address": {
            "@type": "PostalAddress",
            "streetAddress": "Lorenzo Luciano Zacarías y Río Paraguay. Ka'aguy Rory",
            "addressLocality": "Encarnación",
             "addressRegion": "Itapúa",
             "containedInPlace": "Universidad Nacional de Itapúa",
            "addressCountry": "PY"
          }
        },
      }
    </script>
  </head>
  <body>
    <Header />
    <slot />
    <Footer />
  </body>
</html>

